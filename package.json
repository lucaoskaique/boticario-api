  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/shared/http/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:create": "ts-node-dev ./node_modules/typeorm/cli.js  migration:create",
    "typeorm:run": "ts-node-dev ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts migration:run",
    "typeorm:show": "ts-node-dev ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts migration:show",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.0",
    "express-async-errors": "^3.1.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "*"
  }
}
