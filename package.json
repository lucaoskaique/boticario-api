{
  "name": "boticario-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "lucaoskaique <lucas.kaique175@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:create": "ts-node-dev ./node_modules/typeorm/cli.js  migration:create",
    "typeorm:run": "ts-node-dev ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts migration:run",
    "typeorm:show": "ts-node-dev ./node_modules/typeorm/cli.js -d ./src/typeorm/data-source.ts migration:show",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "glob -c \"node --loader tsx --no-warnings --test\" \"./src/modules/**/useCase/**/*.test.ts\"",
    "test:jest": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed:category": "ts-node-dev -r tsconfig-paths/register  src/shared/infra/typeorm/seed/categories.ts",
    "seed:address": "ts-node-dev -r tsconfig-paths/register  src/shared/infra/typeorm/seed/addresses.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1584.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.0",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "3.5.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "*"
  }
}
